{
  "name": "tetrodotoxin-lsp",
  "description": "Language Server for Tetrodotoxin.",
  "categories": ["Programming Languages"],
  "displayName": "Tetrodotoxin",
  "publisher": "Perimortem",
  "author": "Matt Kaes",
  "license": "MIT",
  "icon": "icon.png",
  "version": "0.1.0",
  "pricing": "Free",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatthewKaes/Perimortem"
  },
  "homepage": "https://github.com/MatthewKaes/Perimortem/blob/main/README.md",
  "activationEvents": [
    "onLanguage:tetrodotoxin"
  ],
  "main": "./out/extension",
  "editor.semanticHighlighting.enabled": true,
  "contributes": {
    "languages": [
      {
        "id": "tetrodotoxin",
        "aliases": [
          "Tetrodotoxin"
        ],
        "extensions": [
          ".ttx"
        ],
        "icon": {
          "light": "./icon.svg",
          "dark": "./icon.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "tetrodotoxin",
        "scopeName": "source.tetrodotoxin",
        "path": "./syntaxes/tetrodotoxin.tmGrammar.json"
      }
    ],
    "semanticTokenTypes": [
